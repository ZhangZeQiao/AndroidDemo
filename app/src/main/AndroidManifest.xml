<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zzq.demo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.zzq.demo.done2system.kotlin.KotlinActivity" />
        <activity android:name="com.zzq.demo.done2system.kotlin.SingletonKtActivity" />
        <activity android:name=".done2ui.anim.TurnRoundActivity" />
        <activity android:name=".done2function.sqlite.SQLiteActivity" />

        <service android:name=".done2system.service.MyService" />

    </application>

</manifest>